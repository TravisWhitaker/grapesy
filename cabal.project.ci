packages: 
    ./grpc-spec
  , ./grapesy
  , ./tutorials/quickstart
  , ./tutorials/basics
  , ./tutorials/lowlevel
  , ./tutorials/metadata

package grpc-spec
  tests: True
  flags: +snappy
  ghc-options: -Werror

package grapesy
  tests: True
  benchmarks: True
  flags: +build-demo +build-stress-test 
  ghc-options: -Werror

--
-- ghc 9.10
--

allow-newer: proto-lens:base
allow-newer: proto-lens-runtime:base
